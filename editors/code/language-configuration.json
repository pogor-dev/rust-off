{
    "comments": {
        "lineComment": "%"
    },
    "brackets": [
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "<",
            ">"
        ],
        [
            "<<",
            ">>"
        ],
    ],
    "autoClosingPairs": [
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "<",
            "close": ">"
        }
    ],
    "surroundingPairs": [
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "<",
            "close": ">"
        }
    ],
    "folding": {
        "markers": {
            "start": "\\b(obj|stream)\\b",
            "end": "\\b(endobj|endstream)\\b"
        }
    }
}